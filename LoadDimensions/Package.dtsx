    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        LoadCategories(Row.CategoryID, Row.CategoryName).GetAwaiter();
    }

    public async Task LoadCategories(int categoriesId, string categoriesName)
    {
        try
        {
            using (SqlConnection connection = new SqlConnection(this.Variables.MyCon))
            {
                using (SqlCommand command = new SqlCommand("dbo.LoadCategories", connection))
                {
                    command.Parameters.AddWithValue("@CategoryId", categoriesId);
                    command.Parameters.AddWithValue("@CategoryName", categoriesName);

                    command.CommandType = CommandType.StoredProcedure;

                    await connection.OpenAsync();
                    await command.ExecuteNonQueryAsync();


                }

            }

        }
        catch (Exception ex) 
        {
            Console.WriteLine($"Error procesando las Categories: {ex.Message}");
        }
    }

